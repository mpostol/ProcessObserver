        <bindingRedirect oldVersion="0.0.0.0-2.1.1062.39746" newVersion="2.1.1062.39746" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive.Core" publicKeyToken="94bc3704cddfc263" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.3000.0" newVersion="3.0.3000.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="CAS.CommServer.CommonBus" publicKeyToken="8832ff1a67ea61a3" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.1.6.38674" newVersion="5.1.6.38674" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
